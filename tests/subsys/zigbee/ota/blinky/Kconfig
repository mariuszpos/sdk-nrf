menu "Blinky Sample Settings"

config BLINKY_LED_MASK
	hex "Mask of DK LEDs"
	range 0x1 0xF
	default 0xF
	help
	  The default setting blinks all LEDs.

endmenu

menu "Zephyr Kernel"
source "Kconfig.zephyr"
endmenu
